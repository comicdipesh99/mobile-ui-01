.posts {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 3px;
  padding-top: 10px;
  overflow: scroll;
}

.post {
  /* height: 302px; */
  width: 100%;
  position: relative;
  background: #000;
  z-index: -1;
}

.post img {
  width: inherit;
  display: block;
  margin: auto;
}

.post-user-coins {
  width: 60%;
  display: flex;
  position: absolute;
  top: 0;
  left: 0;
  padding: 10px;
  align-items: center;
  justify-content: space-between;
}

.post-user-coins svg {
  height: 16px;
  width: 16px;
}

.post-user-coins span {
  font-size: 10px;
  font-weight: 600;
  color: #FFFFFF;
}

.post-user-data {
  display: flex;
  position: absolute;
  bottom: 10px;
  left: 10px;
}

.post-user-img {
  height: 34px;
  width: 34px;
  overflow: hidden;
  border-radius: 50%;
}

.post-user-img img {
  display: inline-block;
  width: 100%;
  height: auto;
  border: 1px solid white;
  margin-top: -30%;
}

.user-data {
  padding-left: 10px;
}

h3 {
  font-size: 14px;
  font-weight: 500;
  color: #FFFFFF;
}

h4 {
  font-size: 12px;
  color: #FFFFFF;
  font-weight: 300;
}

.live#live {
  display: flex;
  height: 76px;
  width: 76px;
  justify-content: center;
  border-radius: 50%;
  align-items: center;
  bottom: 10%;
  background: linear-gradient(302deg, #A831FF, #00B3FF);
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
}

.live svg {
  height: 19.65px;
  width: 34.39px;
}

.footer {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 8%;
  width: 100vw;
  bottom: 0;
  position: fixed;
  background-color: #FFFFFF;
}

.footer-btn {
  padding: 15px;
}

.footer-btn svg {
  width: 26px;
  height: 26px;
}









